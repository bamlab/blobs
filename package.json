{
  "name": "blobs",
  "version": "1.1.1",
  "description": "svg blob generator",
  "author": "g-harel",
  "license": "MIT",
  "main": "index.ts",
  "types": "index.d.ts",
  "scripts": {
    "prepack": "npm run build",
    "postpublish": "npm run clean",
    "build": "npm run clean && rollup -c rollup.config.ts",
    "clean": "trash '**/*.js' '**/*.js.map' '**/*.d.ts' '!**/node_modules/**/*'",
    "fmt": "prettier --list-different --write --ignore-path .gitignore '**/*.{js,ts}'",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "prettier": "^1.14.2",
    "rollup": "^1.1.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-plugin-uglify": "^6.0.1",
    "trash-cli": "^1.4.0",
    "ts-jest": "^24.0.2",
    "tslib": "^1.9.3",
    "typescript": "^3.1.3"
  },
  "homepage": "https://github.com/g-harel/blobs#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/g-harel/blobs"
  },
  "bugs": {
    "url": "https://github.com/g-harel/blobs/issues"
  },
  "keywords": [
    "svg",
    "blob"
  ],
  "prettier": {
    "tabWidth": 4,
    "printWidth": 100,
    "trailingComma": "all",
    "bracketSpacing": false,
    "arrowParens": "always"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
